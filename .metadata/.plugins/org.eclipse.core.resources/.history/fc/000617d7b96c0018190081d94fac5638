import org.testng.annotations.Test;

import automation_testBase.testBase;
import automation_uiActions.HomePage;

public class TC002_verifyRegisteration extends testBase{
	
	public static final Logger log=Logger.getLogger(TC002_verifyRegisteration.class.getName());
	HomePage homepage;
	
	@BeforeTest
	public void setup() {
		init();
	}
	
	@Test
	public void createaccount() {
		homepage=new HomePage(driver);
		homepage.signup("test0609@gmail.com", "Manoj", "Kumar", "password", "Manoj", "Kumar", "101 main st", "Manchester", "CT", "06042", "8124567890", "HomeAddress");
	}
	
	@AfterTest
	public void endTest() {
		driver.close();
	}
}
